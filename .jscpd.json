{
  "threshold": 0,
  "reporters": ["console"],
  "ignore": [
    "**/*.json",
    "**/*.md",
    "**/node_modules/**",
    "**/.next/**",
    "**/dist/**",
    "**/build/**"
  ],
  "minLines": 5,
  "minTokens": 50,
  "format": ["typescript", "javascript", "tsx", "jsx"],
  "blame": false,
  "silent": false
}
